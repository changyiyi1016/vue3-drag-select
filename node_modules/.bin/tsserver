#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/bin/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/typescript@4.6.4/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules:/Users/changyi/gitHub_local/node_modules:/Users/changyi/node_modules:/Users/node_modules:/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/typescript/bin/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/typescript/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/bin/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/typescript@4.6.4/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules:/Users/changyi/gitHub_local/node_modules:/Users/changyi/node_modules:/Users/node_modules:/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/typescript/bin/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/typescript/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi

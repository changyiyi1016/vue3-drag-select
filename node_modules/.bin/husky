#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/husky@8.0.1/node_modules/husky/lib/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/husky@8.0.1/node_modules/husky/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/husky@8.0.1/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules:/Users/changyi/gitHub_local/node_modules:/Users/changyi/node_modules:/Users/node_modules:/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/husky/lib/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/husky/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/husky@8.0.1/node_modules/husky/lib/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/husky@8.0.1/node_modules/husky/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/husky@8.0.1/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules:/Users/changyi/gitHub_local/node_modules:/Users/changyi/node_modules:/Users/node_modules:/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/husky/lib/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/husky/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../husky/lib/bin.js" "$@"
else
  exec node  "$basedir/../husky/lib/bin.js" "$@"
fi

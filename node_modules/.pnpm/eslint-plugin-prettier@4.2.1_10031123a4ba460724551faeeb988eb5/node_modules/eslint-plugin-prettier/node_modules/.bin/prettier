#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/prettier@2.7.1/node_modules/prettier/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/prettier@2.7.1/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules:/Users/changyi/gitHub_local/node_modules:/Users/changyi/node_modules:/Users/node_modules:/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/eslint-plugin-prettier@4.2.1_10031123a4ba460724551faeeb988eb5/node_modules/eslint-plugin-prettier/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/eslint-plugin-prettier@4.2.1_10031123a4ba460724551faeeb988eb5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/prettier@2.7.1/node_modules/prettier/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/prettier@2.7.1/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules:/Users/changyi/gitHub_local/node_modules:/Users/changyi/node_modules:/Users/node_modules:/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/eslint-plugin-prettier@4.2.1_10031123a4ba460724551faeeb988eb5/node_modules/eslint-plugin-prettier/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/eslint-plugin-prettier@4.2.1_10031123a4ba460724551faeeb988eb5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prettier@2.7.1/node_modules/prettier/bin-prettier.js" "$@"
else
  exec node  "$basedir/../../../../../prettier@2.7.1/node_modules/prettier/bin-prettier.js" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/resolve@1.22.1/node_modules/resolve/bin/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/resolve@1.22.1/node_modules/resolve/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/resolve@1.22.1/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules:/Users/changyi/gitHub_local/node_modules:/Users/changyi/node_modules:/Users/node_modules:/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/vite@3.0.7/node_modules/vite/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/vite@3.0.7/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/resolve@1.22.1/node_modules/resolve/bin/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/resolve@1.22.1/node_modules/resolve/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/resolve@1.22.1/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules:/Users/changyi/gitHub_local/node_modules:/Users/changyi/node_modules:/Users/node_modules:/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/vite@3.0.7/node_modules/vite/node_modules:/Users/changyi/gitHub_local/vue3-drag-select/node_modules/.pnpm/vite@3.0.7/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../resolve@1.22.1/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../resolve@1.22.1/node_modules/resolve/bin/resolve" "$@"
fi
